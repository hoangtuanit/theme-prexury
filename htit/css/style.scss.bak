@import 'partial/fonts.scss';

/****************BEGIN SETTING***************/
	*{
		font-size: 18px;
	}
	@mixin transform50(){
		position: absolute;		
		top:50%;
		left: 50%;
		transform: translate(-50%,-50%);
		-webkit-transform: translate(-50%,-50%);
	}
	@mixin blue_gradient(){
		background: rgba(43,163,253,1);
		background: -moz-linear-gradient(left, rgba(43,163,253,1) 0%, rgba(19,106,251,1) 100%);
		background: -webkit-gradient(left top, right top, color-stop(0%, rgba(43,163,253,1)), color-stop(100%, rgba(19,106,251,1)));
		background: -webkit-linear-gradient(left, rgba(43,163,253,1) 0%, rgba(19,106,251,1) 100%);
		background: -o-linear-gradient(left, rgba(43,163,253,1) 0%, rgba(19,106,251,1) 100%);
		background: -ms-linear-gradient(left, rgba(43,163,253,1) 0%, rgba(19,106,251,1) 100%);
		background: linear-gradient(to right, rgba(43,163,253,1) 0%, rgba(19,106,251,1) 100%);
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#2ba3fd', endColorstr='#136afb', GradientType=1 );
	}
	@mixin blue_gradient_hover(){
		background: rgba(19,106,251,1);
		background: -moz-linear-gradient(left, rgba(19,106,251,1) 0%, rgba(43,163,253,1) 100%);
		background: -webkit-gradient(left top, right top, color-stop(0%, rgba(19,106,251,1)), color-stop(100%, rgba(43,163,253,1)));
		background: -webkit-linear-gradient(left, rgba(19,106,251,1) 0%, rgba(43,163,253,1) 100%);
		background: -o-linear-gradient(left, rgba(19,106,251,1) 0%, rgba(43,163,253,1) 100%);
		background: -ms-linear-gradient(left, rgba(19,106,251,1) 0%, rgba(43,163,253,1) 100%);
		background: linear-gradient(to right, rgba(19,106,251,1) 0%, rgba(43,163,253,1) 100%);
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#136afb', endColorstr='#2ba3fd', GradientType=1 );
	}
	@mixin orange_gradient(){
		background: rgba(255,126,31,1);
		background: -moz-linear-gradient(left, rgba(255,126,31,1) 0%, rgba(255,92,75,1) 100%);
		background: -webkit-gradient(left top, right top, color-stop(0%, rgba(255,126,31,1)), color-stop(100%, rgba(255,92,75,1)));
		background: -webkit-linear-gradient(left, rgba(255,126,31,1) 0%, rgba(255,92,75,1) 100%);
		background: -o-linear-gradient(left, rgba(255,126,31,1) 0%, rgba(255,92,75,1) 100%);
		background: -ms-linear-gradient(left, rgba(255,126,31,1) 0%, rgba(255,92,75,1) 100%);
		background: linear-gradient(to right, rgba(255,126,31,1) 0%, rgba(255,92,75,1) 100%);
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ff7e1f', endColorstr='#ff5c4b', GradientType=1 );
	}
	@mixin orange_gradient_hover(){
		background: rgba(255,92,75,1);
		background: -moz-linear-gradient(left, rgba(255,92,75,1) 0%, rgba(255,126,31,1) 100%);
		background: -webkit-gradient(left top, right top, color-stop(0%, rgba(255,92,75,1)), color-stop(100%, rgba(255,126,31,1)));
		background: -webkit-linear-gradient(left, rgba(255,92,75,1) 0%, rgba(255,126,31,1) 100%);
		background: -o-linear-gradient(left, rgba(255,92,75,1) 0%, rgba(255,126,31,1) 100%);
		background: -ms-linear-gradient(left, rgba(255,92,75,1) 0%, rgba(255,126,31,1) 100%);
		background: linear-gradient(to right, rgba(255,92,75,1) 0%, rgba(255,126,31,1) 100%);
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ff5c4b', endColorstr='#ff7e1f', GradientType=1 );
	}
	@mixin transition(){
		transition: all 0.5s ease;
		-webkit-transition: all 0.5s ease;
	}
	@mixin box_shadow(){
		box-shadow: 0px 0px 51px 0px rgba(0,0,0,0.08), 0px 6px 18px 0px rgba(0,0,0,0.05);
		-webkit-box-shadow: 0px 0px 51px 0px rgba(0,0,0,0.08), 0px 6px 18px 0px rgba(0,0,0,0.05);
		@include transition();
	}
	@mixin box_shadow_hover(){
		box-shadow: 1px 2px 20px #00000008, -1px 1px 20px #00000008, 0px -2px 20px #0000001f;
		-webkit-box-shadow: 1px 2px 20px #00000008, -1px 1px 20px #00000008, 0px -2px 20px #0000001f;
		@include transition();
	}
	@mixin transform_y50(){
		position: absolute;
		top:50%;
		transform: translateY(-50%);
		-webkit-transform: translateY(-50%);
	}


	@mixin transform_x50(){
		position: absolute;
		left:50%;
		transform: translateX(-50%);
		-webkit-transform: translateX(-50%);
	}


	@mixin pdt_mgt( $px: 10px ){
		padding-top: $px;
		margin-top: $px;
	}

	@mixin pdb_mgb( $px: 10px ){
		padding-bottom: $px;
		margin-bottom: $px;
	}
/****************END SETTINGS  ***************/

/****************BEGIN GLOBAL***************/
