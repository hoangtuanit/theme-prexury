	.circle-radius{
		position: relative;
		height: 2px;
		width: 100%;
		background: red;
		.mini-circle, .large-circle{
			position: absolute;
			display: inline-block;
			content: '';
			border-radius: 50%;
			@include blue_gradient();
			@include transition();
			z-index: 9;		
			display: none;
			&:hover{
				@include blue_gradient_hover();
				@include transition();
			}
		}

		@media ( min-width: 768px ){
			.mini-circle, .large-circle{
				display: inline-block;
			}
			.mini-circle{
				width: 30px;
				height: 30px;
				top: 62px;
				right: 100px;
			}
			.large-circle{
				width: 100px;
				height: 100px;
				right: 15px;
				top: 100px;;;;
			}
		}
		@media ( min-width: 991px ){
		}
		@media ( min-width: 1280px ){
			.mini-circle{
				width: 40px;
				height: 40px;
				top: 62px;
				right: 160px;
			}
			.large-circle{
				width: 160px;
				height: 160px;
				right: 15px;
				top: 100px;;;;
			}
		}
		@media ( min-width: 1536px ){
			.mini-circle{
				width: 50px;
				height: 50px;
				top: 55px;
				right: 261px;
			}
			.large-circle{
				width: 261px;
				height: 261px;
				right: 15px;
				top: 100px;;;;
			}
		}

	}

	.orange-gradient{
		@include orange_gradient();
	}

	.hide-map{
		-webkit-clip-path: polygon(20% 0%, 0% 20%, 30% 50%, 0% 80%, 20% 100%, 50% 70%, 80% 100%, 100% 80%, 70% 50%, 100% 20%, 80% 0%, 50% 30%);
		clip-path: polygon(20% 0%, 0% 20%, 30% 50%, 0% 80%, 20% 100%, 50% 70%, 80% 100%, 100% 80%, 70% 50%, 100% 20%, 80% 0%, 50% 30%);
		height: 40px;
		width: 40px;
		position: absolute;
		z-index: 9;
		right: 15px;
		top: 50%;
		transform: translateY(-50%) ;
		-webkit-transform: translateY(-50%) ;
		background: black;
		display: none;
		&:hover{
			background: #eee;
		}
	}

	.sub-item{
		padding-left: 15px;
		padding-right: 15px;
	}
	// end .owl-sub-category


	.border-left-over{
		position: relative;
		&:hover:before{
			@include transition();
			margin-right: 4px;
		}
		&:before{
			@include transition();
			content:'';
			display: inline-block;
			position: absolute;
			width: 40px;
			height: 2px;
			top: 50%;
			transform: translateY(-50%);
			-webkit-transform: translateY(-50%);
			background: #F9175D;
			right:100%;
			margin-right:10px;
		}
	}
	// end .border-left-over

	.sow-title.border-left-over{
		text-align: left !important;
		position: relative;
		padding-bottom: 0;
		margin-top: 38px;
		margin-bottom: 38px;
		color: #428CFA;
		&:after{
			display: none;
		}
	}

	.main-content{
		img, table,tr ,td,th{
			max-width: 100%;
		}
	}

	.container{
		@media (max-width: 991px){
			max-width: 100%;
		}
	}


/****************END GLOBAL***************/

.sow-title{
	font-family: 'muli';
	font-size: 20px;
	display: block;
	position: relative;
	@include pdb_mgb(20px);
	&.hide_border{
		&:after{
			display: none;
		}
	}
	&:after{
		content: '';
		display: inline-block;
		width: 60px;
		height: 2px;
		bottom: 0;
		@include transform_x50();
		background: #FD8B3A;
	}
	@media ( min-width: 768px ){
		font-size: 24px;
	}
	@media ( min-width: 991px ){
		font-size: 28px;
		@include pdb_mgb(30px);
	}
	@media ( min-width: 1280px ){
		font-size: 32px;
	}
	@media ( min-width: 1536px ){
		font-size: 42px;
	}	
}
// end .sow-title

